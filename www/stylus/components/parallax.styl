.parallax
  height: 736px;
  position: relative;
  &__layer
    background-position: bottom center;
    background-repeat: no-repeat;
    background-size: 1400px
    width: 100%;
    height: 736px;
    position: fixed;
    z-index: -1;
    &-01
      background-image: url('../img/parallax/SurUnBaobab_Parrallax_14.svg');
    &-02
      background-image: url('../img/parallax/SurUnBaobab_Parrallax_12.svg');
