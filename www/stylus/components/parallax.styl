    &-07
      background-image: url('../img/parallax/SurUnBaobab_Parrallax_07.svg');
    &-08
      background-image: url('../img/parallax/SurUnBaobab_Parrallax_06.svg');
    &-09
      background-image: url('../img/parallax/SurUnBaobab_Parrallax_05.svg');
    &-10
